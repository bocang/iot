    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="3dp" />
            <size
                android:height="5px"
                android:width="245px" />
            <gradient android:angle="45" android:startColor="#000000"
                android:endColor="#f4f4f4"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="#00000000" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="3dp" />
                <size
                    android:height="5px"
                    android:width="245px" />
                <gradient android:angle="45" android:startColor="#000000"
                    android:endColor="#f4f4f4"/>
            </shape>
        </clip>
    </item>

</layer-list>
